  <script src="backoffice/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  <script src="backoffice/js/sidebarmenu.js"></script>
  <script src="backoffice/js/app.min.js"></script>
  <script src="backoffice/libs/apexcharts/dist/apexcharts.min.js"></script>
  <script src="backoffice/libs/simplebar/dist/simplebar.js"></script>
  <script src="{{ asset('js/dashboard.js') }}"></script>

  {% endblock %}
  <!--  Scripts End -->
